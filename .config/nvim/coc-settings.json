{
    "git.enableGutters": false,
    "calc.replaceOriginalExpression": false,
    "clangd.semanticHighlighting": true,
    "list.insertMappings": {
        "<C-v>": "action:vsplit",
        "<C-s>": "action:split",
        "<C-t>": "action:tabe",
        "<C-w>": "command:wincmd k"
    },
    "list.source.mru.defaultArgs": [
        "-A"
    ],
    "list.source.files.args": [
        "--no-ignore",
        "--hidden",
        "--files"
    ],
    "suggest.labelMaxLength": 40,
    "snippets.ultisnips.directories": [
        "~/.config/nvim/ultisnips"
    ],
    // coc snippets.editSnippets命令使用的目录,win7下无法识别~
    // "snippets.userSnippetsDirectory": "~/.config/nvim/ultisnips",
    "list.maxHeight": 15,
    "codeLens.enable": true,
    "suggest.languageSourcePriority": 99,
    "coc.source.file.priority": 50,
    "coc.source.around.priority": 40,
    "coc.source.buffer.priority": 30,
    "coc.source.syntax.priority": 10,
    "coc.source.dictionary.priority": 8,
    "coc.source.tag.priority": 6,
    "tabnine.priority": 4,
    "suggest.lowPrioritySourceLimit": 10,
    "suggest.triggerAfterInsertEnter": false,
    "suggest.snippetIndicator": " ►",
    "suggest.minTriggerInputLength": 2,
    "suggest.noselect": false,
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    }
}
